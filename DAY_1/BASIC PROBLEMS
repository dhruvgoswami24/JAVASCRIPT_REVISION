//  1  Sum of two numbers entered by the user.

let num1 = Number(prompt("enter number 1 "))
let num2 = Number(prompt("enter number 2 "))
console.log(num1+num2)



// 2 Swap two numbers using a temporary variable.

var num1 = 5 ; 
var num2 = 10;
var temp ; 
temp = num1 ;
num1 = num2;
num2 = temp;


//  3 Convert Celsius to Fahrenheit.

var temp = Number(prompt("enter temperature in celsius"))
var far = ((temp*(9/5))+32)
console.log(far)



//  4 Find the largest of three numbers.

var num = 1245
var num2 = 50 
var num3 = 100
if(num>num2){
    console.log(num)
}
else if(num2>num3){
    console.log(num2)
}
else {
    console.log(num3)
}
