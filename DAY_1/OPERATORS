//  1.Add, subtract, multiply, divide two numbers.


var num1 = 5
var num2 = 10
console.log(num1+num2);
console.log(num1-num2);
console.log(num1*num2);
console.log(num1/num2);




//  2 Use ++ and -- on a variable and print the result each time.

var num = 5 
console.log(num++)
console.log(++num)
console.log(num--)
console.log(--num)


//  3 Find remainder and exponentiation of two numbers.


var num = 70
var num2 = 100 
console.log(`the remainder is ${num2 % num}`)
console.log(`the exponentiation is ${num2 ** num}`)


// 4 Use ++ and -- on a variable and print the result each time.

